{
  "name": "pharaxe/bensweedler.com",
  "version": 2.0.0,
  "type": "project",
  "license": "MIT",
  "description": "Ben's personal site",
  "homepage": "https://bensweedler.com/",
  "authors": [
    {
      "name": "Ben Sweedler",
      "email": "bsweedler@gmail.com",
      "homepage": "https://github.com/pharaxe"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://composer.deliciousbrains.com"
    },
    {
      "type": "git",
      "url": "https://github.com/pharaxe/salient"
    }
  ],
  "require": {
    "php": ">=7.1",
    "composer/installers": "^1.7",
    "vlucas/phpdotenv": "^3.4.0",
    "oscarotero/env": "^1.2.0",
    "roots/wordpress": "5.2.3",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/akismet": "^4.0.0",
    "wpackagist-plugin/code-syntax-block": "^1.0.1",
    "wpackagist-plugin/easy-add-thumbnail": "^1.1.3",
    "wpackagist-plugin/jetpack": "^7.8.0",
    "wpackagist-plugin/public-post-preview": "^2.8.0",
    "wpackagist-plugin/redirection": "^4.4.2",
    "wpackagist-plugin/safe-svg": "^1.9.4",
    "wpackagist-plugin/social-media-feather": "^1.9.4",
    "wpackagist-plugin/ssl-insecure-content-fixer": "^2.7.2",
    "deliciousbrains-plugin/wp-migrate-db-pro": "*",
    "themenectar/salient": "11.0.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4.2",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
